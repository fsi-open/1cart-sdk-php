{
    "name": "1cart/sdk-php",
    "license": "MIT",
    "type": "library",
    "description": "A PHP SDK used to communicate with 1cart API",
    "autoload": {
        "psr-4": { "OneCart\\Api\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "OneCart\\Tests\\Api\\": "tests" }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://git.fsi.pl"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-fileinfo": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "commerceguys/addressing": "^1.2",
        "commerceguys/enum": "^1.0",
        "egulias/email-validator": "^3.1",
        "giggsey/libphonenumber-for-php": "^8.12",
        "moneyphp/money": "^3.1||^4.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "ramsey/uuid": "^4.2",
        "symfony/mime": "^4.4||^5.0||^6.0"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "^2.8",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "bin-dir": "vendor/bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "suggests": {
        "php-http/httplug": "Provides adapters for different HTTP libraries"
    }
}
